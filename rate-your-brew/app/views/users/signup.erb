<h2> Create a Profile </h2>

<form action='/users/signup' method='POST'>
    Username: <input type='text' name='username'><br>
    Password: <input type='password' name='password'><br>
    <input type='submit'>
</form>

<div class="text-danger">
    <%if flash[:error] %>
 <h3><%= flash[:error] %></h3>
    <% end %>
</div>
